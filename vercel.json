{
    "headers": [
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/favicon.avif",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
            
        }
    ],
    "redirects": [
        {
            "source": "/ads.txt",
            "destination": "https://rev.iq/howbazaar.gg/ads.txt",
            "permanent": true
        }
    ]
}